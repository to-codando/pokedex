{
  "compilerOptions": {
    "baseUrl": "./src",
     "lib": [ "ES2021", "DOM" ],
    "paths": {
      "@/store": ["store/index.ts"],
      "@/components/*": ["components/*"],
      "@/utils/*": ["utils/*"],
      "@/services/*": ["services/*"],
      "@/assets/*": ["assets/*"],
    },
    "strict":true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  }
}
